Algoritmo Ejemplo9
    // Verificar si una prenda tiene descuento
    Escribir("Bienvenido al almacén de ropa")
    Escribir("Ingrese el nombre de la prenda:")
    Leer(nombre)
    
    si nombre = "Camisa" o nombre = "Pantalón" entonces
        Escribir("La prenda tiene descuento")
